{
  "services": [
    {
      "name": "kibana",
      "address": "",
      "port": {{ kibana_port }},
      "checks": [
        {
          "http": "http://{{ bind_address }}:{{ kibana_port }}",
          "interval": "10s",
          "timeout": "3s"
        }
      ],
      "connect": {
        "proxy": {
          "config": {
            "upstreams": [{
              "destination_name": "elastic",
              "destination_type": "nearest_query",
              "local_bind_port": 29200
            }]
          }
        }
      }
    }
  ]
}
